PROMPT 1
DEFAULT robot64
TIMEOUT 1
LABEL robot64
        kernel robot64/vmlinuz
        append debug boot=nfs root=/dev/nfs netboot=nfs nfsroot=10.68.0.1:/,rw ip=10.68.0.2:10.68.0.1:10.68.0.1:255.255.255.0:c2:rename3 selinux=0 initrd=robot64/initrd.img rw pci=acpi rootdelay=10 vga=extended init=/usr/sbin/c2init console=ttyS0,57600 console=tty1 --
label bmc106
        kernel memdisk
        append initrd=disk-bios3a03-bmc106-wg0.1.img
