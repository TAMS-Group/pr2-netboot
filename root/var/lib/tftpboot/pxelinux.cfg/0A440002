PROMPT 1
DEFAULT robot64
TIMEOUT 3
LABEL robot64
        kernel robot64/vmlinuz
        append boot=nfs root=/dev/nfs initrd=robot64/initrd.img rw rootdelay=10 acpi=off numa=noacpi vga=extended nfsroot=10.68.0.1:/,rw ip=dhcp noefi nomodeset init=/usr/sbin/c2init console=ttyS0,57600 console=tty1 
label bmc106
        kernel memdisk
        append initrd=disk-bios3a03-bmc106-wg0.1.img
